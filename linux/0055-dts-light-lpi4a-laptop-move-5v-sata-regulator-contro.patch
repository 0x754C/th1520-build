From 21c2ea2104cffd0ec74c1ac642f727c9f7cb2880 Mon Sep 17 00:00:00 2001
From: Lu Hui <luhui@sipeed.com>
Date: Tue, 5 Dec 2023 15:58:50 +0800
Subject: [PATCH 55/55] dts: light-lpi4a-laptop: move 5v sata regulator control
 into userspace

---
 arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts | 12 ------------
 1 file changed, 12 deletions(-)

diff --git a/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts b/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
index 667bf69ef..57603c393 100644
--- a/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
+++ b/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
@@ -63,18 +63,6 @@ reg_usb_vcc_5v: regulator-usb-vcc-5v-en {
 		regulator-always-on;
         };
 
-	reg_sata_vcc_5v: regulator-sata-vcc-5v-en {
-		status = "okay";
-		compatible = "regulator-fixed";
-		regulator-name = "regulator-sata-vcc-5v-en";
-		regulator-min-microvolt = <5000000>;
-		regulator-max-microvolt = <5000000>;
-		vin-supply = <&reg_sys_vcc_5v>;
-		gpio = <&pcal6408ahk_c 1 1>;
-		enable-active-high;
-		regulator-always-on;
-	};
-
 	audio_amp: regulator-audio-amp-en {
                 status = "okay";
                 compatible = "regulator-fixed";
-- 
